{
  "name": "nest-demo",
  "version": "0.0.0",
  "description": "",
  "author": "Plasticine-Yang",
  "license": "MIT",
  "scripts": {
    "nest:start": "cross-env NODE_ENV=development nest start",
    "nest:dev": "cross-env NODE_ENV=development nest start --watch",
    "nest:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "nest:build": "cross-env NODE_ENV=production nest build",
    "nest:prod": "cross-env NODE_ENV=production node dist/main",
    "env-dev:init": "docker compose -f ./docker-compose.dev.yaml -p gzhu_library_booking_dev up -d",
    "env-dev:start": "docker compose -f ./docker-compose.dev.yaml -p gzhu_library_booking_dev start",
    "env-dev:stop": "docker compose -f ./docker-compose.dev.yaml -p gzhu_library_booking_dev stop",
    "env-dev:clean": "docker compose -f ./docker-compose.dev.yaml -p gzhu_library_booking_dev down -v",
    "deploy": "pnpm env-prod:init && pnpm nest:build && pnpm nest:prod",
    "env-prod:init": "docker compose -f ./docker-compose.prod.yaml -p gzhu_library_booking up -d",
    "env-prod:start": "docker compose -f ./docker-compose.prod.yaml -p gzhu_library_booking start",
    "env-prod:stop": "docker compose -f ./docker-compose.prod.yaml -p gzhu_library_booking stop",
    "env-prod:clean": "docker compose -f ./docker-compose.prod.yaml -p gzhu_library_booking down -v",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier -w",
    "format:src": "prettier -w src",
    "test": "vitest",
    "test:u": "vitest -u"
  },
  "dependencies": {
    "@gzhu-library-booking/core": "workspace:^",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/typeorm": "^9.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "0.14.0",
    "error-stack-parser": "^2.1.4",
    "joi": "^17.9.2",
    "mysql2": "3.2.4",
    "nestjs-pino": "^3.2.0",
    "pino-http": "^8.3.3",
    "pino-pretty": "^10.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sonic-boom": "^3.3.0",
    "typeorm": "^0.3.15",
    "yaml": "^2.2.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.4.2",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@plasticine-yang/eslint-config-typescript": "^0.1.6",
    "@types/express": "^4.17.17",
    "@types/node": "18.16.3",
    "@types/supertest": "^2.0.12",
    "cross-env": "^7.0.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1",
    "webpack": "^5.81.0"
  }
}
